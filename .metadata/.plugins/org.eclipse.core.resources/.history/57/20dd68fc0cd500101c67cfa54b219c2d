import java.util.List;
import java.util.Optional;

public interface MascotaService {

    List<Mascota> listar();

    Mascota obtener(Long id);

    Mascota crear(Mascota mascota);

    Mascota actualizar(Long id, Mascota mascota);

    boolean eliminar(Long id);
}
